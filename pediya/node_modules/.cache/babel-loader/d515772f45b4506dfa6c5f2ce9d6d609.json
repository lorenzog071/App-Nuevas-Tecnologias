CA;AACA,KA5BA;AA6BA,IAAA,KA7BA,mBA6BA;AACA,WAAA,OAAA,GAAA,KAAA,OAAA,GAAA,CAAA;AACA,KA/BA;AAgCA,IAAA,aAhCA,2BAgCA;AACA,WAAA,IAAA,GAAA,SAAA;AACA,WAAA,cAAA,GAAA,IAAA;AACA,KAnCA;AAoCA,IAAA,aApCA,2BAoCA;AACA,WAAA,IAAA,GAAA,SAAA;AACA,KAtCA;AAuCA,IAAA,QAvCA,sBAuCA;AACA,WAAA,QAAA,GAAA,KAAA;AACA,KAzCA;AA0CA,IAAA,eA1CA,6BA0CA;AACA,WAAA,OAAA,GAAA,CAAA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,KA7CA;AA8CA,IAAA,cA9CA,4BA8CA;AACA,WAAA,OAAA,GAAA,CAAA;AACA,WAAA,cAAA,GAAA,KAAA;AACA;AAjDA;AAZA,CAAA","sourcesContent":["<template>\n  <div>\n    <appBar\n      @on-home-click=\"homeBody()\"\n      @on-carrito-click=\"carritoBody()\"\n      @on-favoritos-click=\"favoritosBody()\"\n      @on-pedidos-click=\"pedidosBody()\"\n    />\n    <home\n      @on-send-click=\"addCarrito()\"\n      @on-sendfav-click=\"addFavoritos()\"\n      v-if=\"body == 'home'\"\n    />\n\n\n    <!-- CARRITO DE COMPRAS -->\n    <div v-if=\"body == 'carrito'\">\n      <nav class=\"navbar navbar-expand-sm bg-dark navbar-dark\">\n        <a class=\"navbar-brand\" id=\"nav\">  Carrito de Compras  </a>\n      </nav>\n      <ul class=\"list-group\" v-if=\"borrarpedido == true\">\n        <li class=\"list-group-item\">\n          <img id=\"img-carrito\" src=\"./assets/imagen4.jpg\" alt=\"\" />\n          <div class=\"div-text\">\n            <p id=\"text\">Triple con queso</p>\n          </div>\n          <p id=\"cantidad\">{{ element }}</p>\n          <button\n            id=\"res\"\n            type=\"button\"\n            class=\"btn btn-warning\"\n            @click.prevent=\"restar()\"\n          >\n            -\n          </button>\n          <button\n            id=\"eliminar\"\n            type=\"button\"\n            class=\"btn btn-danger\"\n            @click.prevent=\"eliminarCarrito()\"\n          >\n            Eliminar\n          </button>\n          <button\n            id=\"comprar\"\n            type=\"button\"\n            class=\"btn btn-success\"\n            @click.prevent=\"sendToPedidos()\"\n          >\n            Comprar\n          </button>\n          <button\n            id=\"sum\"\n            type=\"button\"\n            class=\"btn btn-warning\"\n            @click.prevent=\"sumar()\"\n          >\n            +\n          </button>\n        </li>\n      </ul>\n    </div>\n\n<!-- FAVORITOS -->\n    <div v-if=\"body == 'favoritos'\">\n      <nav class=\"navbar navbar-expand-sm bg-dark navbar-dark\">\n        <a class=\"navbar-brand\">Favoritos</a>\n      </nav>\n      <ul class=\"list-group\" v-if=\"favFalse == true\">\n        <li class=\"list-group-item\">\n          <img id=\"img-carrito\" src=\"./assets/imagen4.jpg\" alt=\"\" />\n          <p id=\"text\">{{ favArray }}</p>\n          <button\n            id=\"eliminarfav\"\n            type=\"button\"\n            class=\"btn btn-danger\"\n            @click.prevent=\"eliminar()\">\n            Eiminar de favoritos\n          </button>\n        </li>\n      </ul>\n    </div>\n\n\n<!-- MIS PEDIDOS -->\n    <div v-if=\"body == 'pedidos'\">\n      <nav class=\"navbar navbar-expand-sm bg-dark navbar-dark\">\n        <a class=\"navbar-brand\">Mis pedidos</a>\n      </nav>\n      <div  v-if=\"borrarmipedido == true\" >\n        <ul class=\"list-group\">\n          <li class=\"list-group-item\"> Tu pedido de {{ element }} Hamburgesa Triple con queso a sido aceptado\n          </li>\n          <button\n            id=\"volverpedidos\"\n            type=\"button\"\n            class=\"btn btn-success\"\n            @click.prevent=\"sendTocarrito()\">\n            Volver al carrito\n          </button>\n          <button\n            id=\"eliminarpedidos\"\n            type=\"button\"\n            class=\"btn btn-danger\"\n            @click.prevent=\"eliminarpedido()\">\n            Eliminar Pedido\n          </button>\n        </ul>\n      </div>\n    </div>\n\n\n  </div>\n</template>\n\n<script>\nimport appBar from \"./components/AppBar\";\nimport home from \"./views/Home\";\n\nexport default {\n  components: { appBar, home },\n  name: \"app\",\n  data: () => ({\n    body: \"home\",\n    element: 0,\n    favArray: \"\",\n    favFalse: true,\n    borrarpedido: true,\n    borrarmipedido: true,\n  }),\n\n  methods: {\n    homeBody() {\n      this.body = \"home\";\n    },\n    pedidosBody() {\n      this.body = \"pedidos\";\n    },\n    carritoBody() {\n      this.body = \"carrito\";\n    },\n    favoritosBody() {\n      this.body = \"favoritos\";\n    },\n    addCarrito() {\n      this.body = \"carrito\";\n      this.element = this.element + 1;\n      this.borrarpedido = true;\n    },\n    addFavoritos() {\n      if (this.favFalse == false) {\n        this.favFalse = true;\n      }\n      this.favArray = \"Triple con queso\";\n    },\n    restar() {\n      if (this.element > 1) {\n        this.element = this.element - 1;\n      }\n    },\n    sumar() {\n      this.element = this.element + 1;\n    },\n    sendToPedidos() {\n      this.body = \"pedidos\";\n      this.borrarmipedido = true;\n    },\n    sendTocarrito() {\n      this.body = \"carrito\";\n    },\n    eliminar() {\n      this.favFalse = false;\n    },\n    eliminarCarrito() {\n      this.element = 0\n      this.borrarpedido = false;\n    },\n    eliminarpedido() {\n      this.element = 0\n      this.borrarmipedido = false;\n    },\n  },\n};\n</script>\n\n\n<style>\n#img-carrito{\n  width: 100%;\n}\n\n#text{\n  font-size: 23px;\n  text-align: center;\n  margin-top: 7px;\n}\n#cantidad {\n  color: #ffffff; \n  font-size: 23px; \n  text-align: center; \n  background: #3d4049; \n  padding: 10px; \n  border-radius: 35px 0px 35px 0px;\n  margin-top: -13px; \n}\n\n#sum{\n  margin-left: 30px;\n}\n\n#eliminar{\n  margin-left: 35px;\n}\n\n#comprar{\n  margin-left: 65px;\n}\n\n#eliminarfav{\n  margin-left: 28%;\n}\n</style>"],"sourceRoot":"src"}]}